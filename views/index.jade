extends layout
link(rel='stylesheet', href='/stylesheets/style.css')
block content
  body
    div.wrapper
      div.container
        dive#two-columns.grid-container(style='display:block;')
          ul.rig.columns-4
            each image in images
              li
                img(src="/images/#{image.source}" name="original")
                span
                  form(method="POST" action="/view/#{image._id}" enctype="multipart/form-data")
                    input(type="image" src="/images/#{image.source}" name="bigImage" width="300")
                p #{image.score}
